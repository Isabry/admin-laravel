{
	"name": "isabry/admin-laravel",
	"description": "The admin integration for Laravel.",
	"keywords": ["framework", "laravel"],
	"authors": [
		{
			"name": "ISY",
			"email": "isabry@gmail.com"
		}
	],
	"require": {
		"laravel/framework": "4.2.*",
		"barryvdh/laravel-debugbar": "~1.8",
		"cartalyst/sentry": "2.1.*"
	},
	"autoload": {
        "psr-4": {
            "Isabry\\AdminLaravel\\": "src"
        },
        "classmap": [
			"src/commands",
			"src/controllers",
			"src/migrations",
			"src/seeds"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize",
			"php artisan debugbar:publish"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
